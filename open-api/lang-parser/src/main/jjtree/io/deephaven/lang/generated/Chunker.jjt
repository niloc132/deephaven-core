// Do not define new token regex here, unless they are for junk nodes!
// The actual AST nodes defined below

ChunkerDocument Document() :
{
Node n = jjtThis;
}
{
  (
     // Grab as many statements as we can.
     LOOKAHEAD({ groovy })
     EatGroovyStatements()
     |
     LOOKAHEAD({ python })
     EatPythonTopLevel()
  )
  Eof()
    {
      // document is done.  Fill in backlinks.
      jjtThis.jjtGetFirstToken().addBackLinks();
      return jjtThis;
    }
}

Node EatStatements(boolean persistent) #void:
{Node n = null;}
{
  (
     try {
       (
         n = Newline() |
         // We know that leading whitespace is not handled well by this production,
         // but in properly formatted python scripts, we will only ever consider
         // leading whitespace in the properly-formed EatPythonStatements() productions.
         // We'll leave this not-whitespace-wise production here as a fallback;
         // this production will only ever be entered as a fallback when properly formatted parsing fails.
         LOOKAHEAD( { isClassDecl() } )
         (
           LOOKAHEAD( { python } )
           n = PythonClassDecl()
           |
           n = JavaClassDecl()
         )
         | LOOKAHEAD( { python && isDef() })
             n = PythonMethod()
         | LOOKAHEAD( { groovy && isDef() })
             n = PythonMethod()
         |
         n = Whitespace() |
         n = Statement()
       )
       } catch (ParseException e) {
         // Whenever there is an unhandled parse exception,
         // gobble up the rest of the line and attach to previous node.
         if (n != null) {
           token = n.addJunk(eatJunk());
         }
         fixState(token);
         if (!persistent) {
           return n;
         }

       }
     )*
  { return n; }
}

Node EatGroovyStatements() #void:
{
        Node n = null;
        ChunkerAssign assign = null;
 }
{
  (
    Whitespace()
    |
    Newline()
    |
    LOOKAHEAD( <CLASS> )
    JavaClassDecl()
    |
    <METHOD_MOD>
    (
        LOOKAHEAD( <CLASS> )
        JavaClassDecl()
        |
        // A<Typed> field
        LOOKAHEAD( { isTypedAssign() } )
        assign = TypedAssign()
        [ n = Values() {
                  assign.setValue(n);
                  assign.setValue(n);
              }]
        |
        // AnUntyped field
        LOOKAHEAD( { isAssign() } )
        assign = Assign()
        [ n = Values() {
                  assign.setValue(n);
              }]
        |
        // Hm, probably need to handle "public List<Blah> fieldName", which is NOT an assign, above, before handling method def, below
        GroovyMethodDef()
    )
    |
    LOOKAHEAD( <CLASS> )
    JavaClassDecl()
    |
    // Need to handle top-level assignments before we consider methods (to avoid ambiguity w/ method definitions)
    LOOKAHEAD( { isTypedAssign() } )
    assign = TypedAssign()
    [ n = Values() {
              assign.setValue(n);
          }]
    |
    LOOKAHEAD( { isAssign() } )
    assign = Assign()
    [ n = Values() {
              assign.setValue(n);
          }]
    |
    // gahhhh, soooo hard to detect method definitions...
    // since we've eliminated public|private|default, etc, we can now reduce our lookahead to:
    // [ <TypeParams> ] ReturnType[<More, Params>] name(
    LOOKAHEAD( { isTypeParam() })
    GroovyMethodDef()
    |
    // Now, to find method defs from just `ReturnType [<TypeParams>] name(`
    LOOKAHEAD( { isReturnTypeThenInvoke() })
    GroovyMethodDef()
    |
    Statement()
  )*
  { return n; }
}

