| < #DIGIT: [ "0"-"9" ] ( [ "0"-"9", "_" ] )* >
| < #SCIENTIFIC: [ "e", "E" ] ( "-" )? <DIGIT>  >
| < NUM:
    // see https://docs.oracle.com/javase/tutorial/java/nutsandbolts/datatypes.html
    // we'll allow for all valid java formats; will consider python/groovy later
    ( "-" | "+" )?
    (
      // hex
      "0x" [ "0"-"9", "a"-"f", "A"-"F" ] ( [ "0"-"9", "a"-"f", "A"-"F", "_" ] )*
    )
    |
