|  < EXTENDS: "extends" >
|  < IMPLEMENTS: "implements" >
|  < CLASS: ("class" | "interface" | "enum" | "@interface") ( <WHITESPACE> | <NEWLINE> ) >
|  < METHOD_MOD: ( ( "public" | "protected" | "private" | "default" | "static" | "final" | "strictfp" ) ( <WHITESPACE> | <NEWLINE> ) )+ >
|  < SUPER: "super" >
|  < DEF: "def" >
